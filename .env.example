# Docker environment variables
# Copy this file to .env and customize for your environment

# ===========================================
# SQL Server Configuration
# ===========================================

# Password for SQL Server SA account
# Requirements: min 12 chars, uppercase, lowercase, digit, special char
SA_PASSWORD=YourStrong@Password

# ===========================================
# Admin Account Configuration
# ===========================================

# Administrator email (used for login)
ADMIN_EMAIL=admin@x.pl

# Administrator password
# Requirements: min 12 chars, uppercase, lowercase, digit, special char
ADMIN_PASSWORD=Pass1234!@#$

# ===========================================
# Application Settings
# ===========================================

# Whether to seed test data (users, resources, bookings)
# Set to 'false' for production environments
SEED_TEST_DATA=true
